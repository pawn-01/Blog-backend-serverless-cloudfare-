
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "prisma..accerate url after attach database to prisma"
  // url = "postgresql.url"
}

model user{
  id Int @id @default(autoincrement())
  username String
  email String @unique
  password String
  post blog[]
}

model blog{
  id Int @id @default(autoincrement())
  userid Int 
  User user @relation(fields: [userid],references: [id])
  tittle String
  blog String
}
